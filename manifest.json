{
    "manifest_version": 2,
    "name": "Jussi PR Manager",
    "version": 1,
    "description": "Extension to easily view Jussi Plataformas Pull Requests Labels",
    "author": "Matheus Gomide",
    "default_locale": "pt-br",
    "icons": {
        "16":   "icons/icon-16.png",
        "48":   "icons/icon-48.png",
        "128":  "icons/icon-128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon-48.png",
        "default_title": "Jussi PRs Manager",
        "default_popup": "popup.html"
    }
}